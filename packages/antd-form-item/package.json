{
  "name": "@fox-design/antd-form-item",
  "version": "0.0.2",
  "description": "foxpage component",
  "keywords": [
    "foxpage",
    "component"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "author": "melody-yangjie",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:lib & npm run build:es & npm run build:foxpage",
    "build:foxpage": "foxpage-cmpt build --foxpage --no-zip-fox",
    "build:lib": "foxpage-cmpt build --lib",
    "build:es": "foxpage-cmpt build --es-module",
    "build:md": "foxpage-cmpt build --schema-md",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "antd": "^4.16.8",
    "core-js": "^3.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@fox-design/antd-button": "*",
    "@fox-design/antd-form": "^0.0.2",
    "@fox-design/antd-input": "*"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "files": [
    "lib/**",
    "es/**",
    "dist/**",
    "*.js",
    "*.d.ts",
    "schema.md"
  ]
}
